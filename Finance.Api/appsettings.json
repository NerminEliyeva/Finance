{
  "Serilog": {
    "Using": "Serilog.Sinks.PostgreSQL",
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
      "WriteTo": [
        {
          "Name": "PostgreSQL",
          "Args": {
            "connectionString": "User ID=postgres;Password=3927;Host=localhost;Port=5432;Database=Finance;Pooling=true;",
            "tableName": "Logs",
            "needAutoCreateTable": true
          }
        }
      ]
    },
    "AllowedHosts": "*"
}

